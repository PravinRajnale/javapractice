import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int i = sc.nextInt();
		int j = sc.nextInt();
		
		for(i=1;i<=4;i++){
		    for(j=1;j<=4;j++){
		        System.out.print("*");
		    }
		    System.out.println();
		}
	}
}


output 
****
****
****
****
